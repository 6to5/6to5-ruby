Gem::Specification.new do |s|
  s.name = '6to5-source'
  s.version = "<%= version %>"
  s.date = Time.at(<%= date %>)

  s.summary = "6to5 source"
  s.homepage = "https://github.com/6to5/6to5-ruby"
  s.license = "MIT"

  s.files = [
    'lib/6to5.js',
    'lib/6to5/polyfill.js',
    'lib/6to5/runtime.js',
    'lib/6to5/source.rb'
  ]

  s.authors = ['Sebastian McKenzie']
  s.email   = 'sebmck@gmail.com'
end
